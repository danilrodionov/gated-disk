     RowBox[{"\[ImaginaryI]", " ", 
      FractionBox[
       RowBox[{"2", "\[Pi]", " ", 
        OverscriptBox["\[Sigma]", "~"]}], 
       OverscriptBox["\[Omega]", "~"]], 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", "1"], "  ", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
