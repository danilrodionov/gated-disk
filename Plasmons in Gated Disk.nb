Cell[TextData[{
 "The average absorption power is given the following integral over the \
surface disk\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", 
      FormBox[
       RowBox[{"\[Integral]", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Re", "[", 
            RowBox[{
             RowBox[{
              StyleBox["j",
               FontWeight->"Bold"], "(", "r", ")"}], 
             SuperscriptBox["\[ExponentialE]", 
              RowBox[{
               RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Omega]", " ", "t"}]]}],
             "]"}], ",", 
           RowBox[{"Re", "[", 
            RowBox[{
             RowBox[{
              StyleBox["j",
               FontWeight->"Bold"], "(", "r", ")"}], " ", 
             SuperscriptBox["\[ExponentialE]", 
              RowBox[{
               RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Omega]", " ", "t"}]]}],
             "]"}]}], ")"}], 
         RowBox[{"\[DifferentialD]", "S"}]}]}],
       TraditionalForm], "\[RightAngleBracket]"}], "=", 
     RowBox[{
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{"\[Integral]", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"Re", "[", 
             RowBox[{
              StyleBox["j",
               FontWeight->"Bold"], 
              StyleBox["(",
               FontWeight->"Plain"], 
              StyleBox["r",
               FontWeight->"Plain"], 
              StyleBox[")",
               FontWeight->"Plain"]}], "]"}], 
            RowBox[{"Re", "[", 
             RowBox[{
              StyleBox["E",
               FontWeight->"Bold"], "(", "r", ")"}], "]"}]}], "+", 
           RowBox[{
            RowBox[{"Im", "[", 
             RowBox[{
              StyleBox["j",
               FontWeight->"Bold"], "(", "r", ")"}], "]"}], 
            RowBox[{"Im", "[", 
             RowBox[{
              StyleBox["E",
               FontWeight->"Bold"], "(", "r", ")"}], "]"}]}]}], ")"}], 
         RowBox[{"\[DifferentialD]", "S"}]}]}]}], "=", 
      RowBox[{
       RowBox[{
        FractionBox["1", "2"], 
        RowBox[{"\[Integral]", 
         RowBox[{
          RowBox[{"Re", "[", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              StyleBox["j",
               FontWeight->"Bold"], "(", "r", ")"}], ",", 
             RowBox[{
              SuperscriptBox[
               StyleBox["E",
                FontWeight->"Bold"], "*"], "(", "r", ")"}]}], ")"}], "]"}], 
          RowBox[{"\[DifferentialD]", "S"}]}]}]}], "=", 
       RowBox[{
        RowBox[{"\[Pi]", " ", 
         SuperscriptBox["R", "2"], 
         RowBox[{
          SubsuperscriptBox["\[Integral]", "0", "1"], 
          RowBox[{"\[Integral]", 
           RowBox[{
            RowBox[{"Re", "[", 
             RowBox[{
              RowBox[{
               SubscriptBox["j", "r"], "(", "r", ")"}], 
              RowBox[{
               SubsuperscriptBox["E", "r", "*"], "(", "r", ")"}]}], "]"}], 
            "r", 
            RowBox[{"\[DifferentialD]", "r"}]}]}]}]}], "=", 
        RowBox[{
         RowBox[{"\[Pi]", " ", 
          SuperscriptBox["R", "2"], 
          RowBox[{
           SubsuperscriptBox["\[Integral]", "0", "1"], 
           RowBox[{"\[Integral]", 
            RowBox[{
             RowBox[{"Re", "[", 
              RowBox[{
               RowBox[{
                SubscriptBox["j", "r"], "(", "r", ")"}], 
               FractionBox[
                RowBox[{
                 SubsuperscriptBox["j", "r", "*"], "(", "r", ")"}], 
                RowBox[{"c", " ", 
                 SuperscriptBox["\[Sigma]", "*"]}]]}], "]"}], "r", 
             RowBox[{"\[DifferentialD]", "r"}]}]}]}]}], "=", 
         RowBox[{
          RowBox[{"\[Pi]", " ", 
           SuperscriptBox["R", "2"], 
           RowBox[{
            SubsuperscriptBox["\[Integral]", "0", "1"], 
            RowBox[{"\[Integral]", 
             RowBox[{
              RowBox[{"Re", "[", 
               RowBox[{
                RowBox[{
                 SubscriptBox["j", "r"], "(", "r", ")"}], 
                FractionBox[
