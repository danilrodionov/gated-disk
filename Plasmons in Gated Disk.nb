 Background->RGBColor[
  0.88, 1, 0.88],ExpressionUUID->"42e09cac-31e6-44c2-8b73-e88fb8c07487"],

Cell[TextData[{
 "We solve the following equation\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      StyleBox["j",
       FontWeight->"Bold"], "(", "r", ")"}], "=", 
     RowBox[{
      RowBox[{"\[Sigma]", " ", 
       RowBox[{
        SuperscriptBox[
         StyleBox["E",
          FontWeight->"Bold"], "ext"], "(", "r", ")"}]}], "+", 
      RowBox[{"\[ImaginaryI]", " ", 
       FractionBox[
        RowBox[{"2", "\[Pi]", " ", "\[Sigma]"}], "\[Omega]"], 
       RowBox[{"(", GridBox[{
          {
           RowBox[{
            RowBox[{
             FractionBox[
              SuperscriptBox["\[PartialD]", "2"], 
              RowBox[{"\[PartialD]", 
               SuperscriptBox["r", "2"]}]], 
             RowBox[{"+", 
              RowBox[{
               FractionBox["1", "r"], " ", 
               FractionBox["\[PartialD]", 
                RowBox[{"\[PartialD]", "r"}]]}]}]}], "-", 
            FractionBox["1", 
             SuperscriptBox["r", "2"]], "+", 
            FractionBox[
             SuperscriptBox["\[Omega]", "2"], 
             SuperscriptBox["c", "2"]]}], "0"},
          {"0", 
           FractionBox[
            SuperscriptBox["\[Omega]", "2"], 
            SuperscriptBox["c", "2"]]}
         }], ")"}], 
       RowBox[{
        SubsuperscriptBox["\[Integral]", "0", "R"], "  ", 
        RowBox[{
         RowBox[{"G", "(", 
          RowBox[{"r", ",", 
           RowBox[{"r", "'"}]}], ")"}], 
         RowBox[{
          StyleBox["j",
           FontWeight->"Bold"], "(", 
          RowBox[{"r", "'"}], ")"}], " ", 
         RowBox[{"r", "'"}], 
         RowBox[{"\[DifferentialD]", 
          RowBox[{"r", "'"}]}]}]}]}]}]}], ","}], TraditionalForm]],
  ExpressionUUID->"920fc6af-0c1d-485c-83a8-37ff773fcc38"],
 "\nwith the boundary conditions ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["j", "r"], "(", "R", ")"}], "=", "0"}], TraditionalForm]],
  ExpressionUUID->"f1fdb22f-900a-478e-8a3a-4346216c6289"],
 ", where\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"G", "(", 
     RowBox[{"r", ",", 
      RowBox[{"r", "'"}]}], ")"}], "=", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
     RowBox[{
      FractionBox["p", 
       SqrtBox[
        RowBox[{
         SuperscriptBox["p", "2"], "-", 
         FractionBox[
          SuperscriptBox["\[Omega]", "2"], 
          SuperscriptBox["c", "2"]]}]]], 
      RowBox[{"(", 
       RowBox[{"1", "-", 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{
          RowBox[{"-", "2"}], 
          SqrtBox[
           RowBox[{
            SuperscriptBox["p", "2"], "-", 
            FractionBox[
             SuperscriptBox["\[Omega]", "2"], 
             SuperscriptBox["c", "2"]]}]], "d"}]]}], ")"}], 
      RowBox[{
       SubscriptBox["J", "1"], "(", 
       RowBox[{"p", " ", "r"}], ")"}], 
      RowBox[{
       SubscriptBox["J", "1"], "(", 
       RowBox[{"p", " ", 
        RowBox[{"r", "'"}]}], ")"}], 
      RowBox[{"\[DifferentialD]", "p"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"f67f0f3d-e249-425b-b6eb-226b3cc788c3"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Sigma]", "=", 
    FractionBox[
     RowBox[{"n", " ", 
      SuperscriptBox["e", "2"], "\[Tau]"}], 
     RowBox[{"m", "(", 
      RowBox[{"1", "-", 
       RowBox[{"\[ImaginaryI]", " ", "\[Omega]", " ", "\[Tau]"}]}], ")"}]]}], 
   TraditionalForm]],ExpressionUUID->"1c2958ef-2d19-43b8-82c3-488dda6399e1"],
 ".\nWe choose the radial external field ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["E", "ext"], TraditionalForm]],ExpressionUUID->
  "2b29437f-96b6-4157-bb11-15533c2004cb"],
 " so for the current components  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["j", "\[Theta]"], "(", "r", ")"}], "=", "0"}], 
   TraditionalForm]],ExpressionUUID->"276de0a5-5178-49ec-9d8c-a7df6990d38b"],
 " and\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["j", "r"], "(", "r", ")"}], "=", 
    RowBox[{
     RowBox[{"\[Sigma]", " ", 
      RowBox[{
       SubsuperscriptBox["\[CapitalEpsilon]", "r", "ext"], "(", "r", ")"}]}], 
     "+", 
     RowBox[{"\[ImaginaryI]", " ", 
      FractionBox[
       RowBox[{"2", "\[Pi]", " ", "\[Sigma]"}], "\[Omega]"], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{
          FractionBox[
           SuperscriptBox["\[PartialD]", "2"], 
           RowBox[{"\[PartialD]", 
            SuperscriptBox["r", "2"]}]], 
          RowBox[{"+", 
           FractionBox["1", "r"]}]}], " ", 
         RowBox[{
          FractionBox["\[PartialD]", 
           RowBox[{"\[PartialD]", "r"}]], 
          RowBox[{"-", 
           FractionBox["1", 
            SuperscriptBox["r", "2"]]}]}]}], "+", 
        FractionBox[
         SuperscriptBox["\[Omega]", "2"], 
         SuperscriptBox["c", "2"]]}], ")"}], 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", "R"], "  ", 
       RowBox[{
        RowBox[{"G", "(", 
         RowBox[{"r", ",", 
          RowBox[{"r", "'"}]}], ")"}], 
        RowBox[{
         SubscriptBox["j", "r"], "(", 
         RowBox[{"r", "'"}], ")"}], " ", 
        RowBox[{"r", "'"}], 
        RowBox[{
         RowBox[{"\[DifferentialD]", 
          RowBox[{"r", "'"}]}], "."}]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"172f50a6-f43f-4257-8652-7cdc50a32fbb"],
 "\nWe expand the current in the following series \n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["j", "r"], "(", "r", ")"}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"n", "=", "1"}], "N"], 
     RowBox[{
      SubscriptBox["C", "n"], 
      FractionBox["r", "R"], 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"1", "-", 
          FractionBox[
           SuperscriptBox["r", "2"], 
           SuperscriptBox["R", "2"]]}], ")"}], "n"], "."}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"a7d9ea1c-9038-4608-ad5d-9f97c8408b19"],
 "\nThese basis polynomials satisfy the boundary condition and the parity of \
the kernel ",
 Cell[BoxData[
  FormBox[
   RowBox[{"G", "(", 
    RowBox[{"r", ",", 
     RowBox[{"r", "'"}]}], ")"}], TraditionalForm]],ExpressionUUID->
  "eeff1410-5c3f-4292-8bcb-21a644547b01"],
 ".\n"
}], "Text",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
 FontSize->14,ExpressionUUID->"20a2b81b-1f11-4f3b-b8ae-a8b642428ed3"],

Cell[TextData[{
 "The system of linear equations for the expansion coefficients ",
